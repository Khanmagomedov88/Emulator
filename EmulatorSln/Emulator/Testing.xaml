<Window x:Class="Emulator.Testing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:hlp="clr-namespace:Helpers"
        xmlns:local="clr-namespace:Emulator"
        mc:Ignorable="d"
        Title="Testing" Height="450" Width="800">
    <Grid>
        <Button Name='But_NavGifN' Width="190" Height="150" Cursor="Hand" HorizontalAlignment="Left"
                VerticalAlignment="Top" Margin="300,120,0,0" Foreground="White"  >
            <MediaElement x:Name="Image_NavGifN" Source="{Binding Source=Resources/gif/ГончаренкоН.mp4}" LoadedBehavior="Manual" UnloadedBehavior="Manual" Height="161" Width="190"></MediaElement>
        </Button>
        <Button Width="190" Cursor="Hand" HorizontalAlignment="Left" Margin="300,282,0,111" Click="Button_Click">Запустить видео</Button>
    </Grid>
</Window>
